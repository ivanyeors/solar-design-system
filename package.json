{
  "name": "solar-design-system-tokens",
  "version": "1.0.0",
  "description": "Token extraction and organization toolkit for Solar Design System",
  "main": "src/tokens/index.js",
  "scripts": {
    "extract": "node token-extractor.js",
    "extract:py": "python token-extractor.py",
    "generate-token-utilities": "node scripts/generate-token-utilities.js",
    "process-tokens": "python scripts/process_tokens.py",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm run process-tokens && npm run generate-token-utilities",
    "build": "vite build",
    "build:gh-pages": "vite build",
    "predev": "npm run process-tokens && npm run generate-token-utilities",
    "dev": "concurrently \"vite\" \"npm run browser-tools\"",
    "browser-tools": "npx @agentdeskai/browser-tools-server@1.2.0"
  },
  "keywords": [
    "design-system",
    "tokens",
    "design-tokens",
    "token-studio",
    "css",
    "theming"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.0",
    "sass": "^1.86.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue": "^3.3.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
